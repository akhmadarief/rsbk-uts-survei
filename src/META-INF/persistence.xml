<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="LocalPersistence">
        <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
        <class>me.lazydev.projects.survei.models.SurveiDataEntity</class>
        <properties>
            <property name="openjpa.ConnectionURL" value="jdbc:mariadb://localhost:3306/survei"/>
            <property name="openjpa.ConnectionDriverName" value="org.mariadb.jdbc.Driver"/>
            <property name="openjpa.ConnectionUserName" value="root"/>
            <property name="openjpa.ConnectionPassword" value=""/>
            <property name="openjpa.Log" value="DefaultLevel=TRACE, Tool=INFO"/>
        </properties>
    </persistence-unit>
</persistence>
